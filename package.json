{
  "name": "joplin-plugin-athena",
  "version": "0.1.1",
  "scripts": {
    "dist": "npm run plugin-build && npm run plugin-extrascripts && npm run plugin-pack",
    "plugin-build": "webpack --config-name pluginConfig",
    "plugin-extrascripts": "webpack --config-name attachContentScripts",
    "plugin-pack": "webpack --config-name createArchiveConfig",
    "prepare": "npm run test && npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "canvas": "^2.8.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "constants-browserify": "^1.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-type": "^16.5.3",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "https-browserify": "^1.0.0",
    "jest": "^27.4.5",
    "linq-to-typescript": "^8.1.0",
    "lint-staged": "^11.2.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pdfjs-dist": "^2.9.359",
    "prettier": "^2.3.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tar": "^6.1.8",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "url": "^0.11.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "worker-loader": "^3.0.8",
    "yargs": "^17.1.1"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "pdfjs-dist": "^2.9.359",
    "ts-node": "^10.4.0"
  }
}
